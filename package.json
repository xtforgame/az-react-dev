{
  "name": "az-react-dev",
  "version": "1.0.0",
  "description": "A react seed project",
  "main": "dist/server/index.js",
  "repository": {
    "type": "git"
  },
  "keywords": [
    "gulp",
    "seed",
    "boilerplate",
    "kickstarter",
    "ES6",
    "6to5"
  ],
  "author": "Rick Chen <xtforgame@gmail.com>",
  "scripts": {
    "lint": "eslint --ext .js ./src",
    "lint-fix": "eslint --fix --ext .js ./src",
    "lint-all": "eslint --ext .js ./src ./test ./internals",
    "lint-all-fix": "eslint --fix --ext .js ./src ./test ./internals",
    "build": "cross-env NODE_ENV=production gulp server:build && cross-env NODE_ENV=production gulp server:build:extras && cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.js",
    "start": "cross-env NODE_ENV=development gulp server:watch:dev",
    "debug": "cross-env NODE_ENV=development gulp server:watch:dev --inspect",
    "test:server": "eslint --ext .js ./test/server && cross-env NODE_ENV=test mocha --require babel-core/register './test/server/**/*.spec.js'",
    "test-watch:server": "eslint --ext .js ./test/server && cross-env NODE_ENV=test mocha --watch ./test --watch-extensions js --require babel-core/register './test/server/**/*.spec.js'",
    "test:front-end": "eslint --ext .js ./test/front-end && cross-env NODE_ENV=test mocha --require ignore-styles --require babel-core/register './test/front-end/**/*.spec.js'",
    "test-watch:front-end": "eslint --ext .js ./test/front-end && cross-env NODE_ENV=test mocha --watch ./test --watch-extensions js --require ignore-styles --require babel-core/register './test/front-end/**/*.spec.js'"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0"
  },
  "dependencies": {
    "app-root-path": "^1.0.0",
    "az-restful-helpers": "git+https://github.com/xtforgame/az-restful-helpers.git#v0.1.0",
    "azldi": "git+https://github.com/xtforgame/azldi.git#v0.1.0",
    "fontfaceobserver": "^2.0.13",
    "generic-router": "git+https://github.com/xtforgame/generic-router.git#v1.2.0",
    "history": "^4.0.0",
    "http-shutdown": "^1.2.0",
    "immutable": "^3.8.1",
    "intl": "^1.2.5",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-observable": "^0.16.0",
    "reselect": "^3.0.1",
    "rrw-module": "git+https://github.com/xtforgame/rrw-module.git#v0.1.3",
    "rxjs": "^5.4.3"
  },
  "devDependencies": {
    "az-gulp-env-lite": "git+https://github.com/xtforgame/az-gulp-env-lite.git#v1.0.0-alpha.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^3.1.4",
    "css-loader": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "^2.0.0",
    "gulp": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "koa-static": "^3.0.0",
    "koa-webpack": "^5.1.0",
    "mocha": "^5.2.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sinon": "^4.3.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.19.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.0"
  },
  "false": {}
}
